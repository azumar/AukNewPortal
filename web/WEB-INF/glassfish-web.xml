<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
  <security-role-mapping>
    <role-name>sourced</role-name>
    <group-name>sourced</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>kukAdmin</role-name>
    <group-name>kukAdmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>stbUser</role-name>
    <group-name>stbUser</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>bankUser</role-name>
    <group-name>bankUser</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>bankAdmin</role-name>
    <group-name>bankAdmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>stbAdmin</role-name>
    <group-name>stbAdmin</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>pume</role-name>
    <group-name>pume</group-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</glassfish-web-app>
